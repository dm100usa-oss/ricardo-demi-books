{
  "system": "FSCBAC_Standard",
  "version": "3.1.0",
  "format": "ULTRA_FORMAL_MANIFEST",
  "purpose": "Canonical machine-readable manifest for the FSCBAC Data Layer and API Layer.",
  "canonical_url": "https://ricardo-demi.com",
  "repository": "https://github.com/dm100usa-oss/ricardo-demi-books",
  "api_base": "https://ricardo-demi-books.vercel.app/api/",
  "data_base": "https://ricardo-demi-books.vercel.app/public/api/",
  "status": "LIVE_AND_OPERATIONAL",

  "description": {
    "summary": "The FSCBAC Standard 3.1.0 defines a deterministic, machine-readable, verifiable and structurally stable system for analyzing and classifying children's books ages 1â€“10. This manifest serves as the authoritative indexing file for AI systems, search engines, research crawlers and automated evaluators.",
    "deterministic_rules": true,
    "immutable_core": true,
    "ai_ready": true
  },

  "structure": {
    "total_models": 15,
    "models": [
      "books.json",
      "ages.json",
      "skills.json",
      "intents.json",
      "book_types.json",
      "value_profile_taxonomy.json",
      "value_profile_rules.json",
      "matching.json",
      "validation.json",
      "awards.json",
      "author.json",
      "query_clusters.json",
      "schema.json",
      "ecl.json",
      "ai_master_structure.json"
    ]
  },

  "model_categories": {
    "immutable": [
      "ages.json",
      "skills.json",
      "intents.json",
      "book_types.json",
      "value_profile_taxonomy.json",
      "value_profile_rules.json",
      "validation.json",
      "ecl.json",
      "schema.json",
      "ai_master_structure.json"
    ],
    "extensible": [
      "books.json",
      "awards.json",
      "author.json",
      "query_clusters.json",
      "matching.json"
    ]
  },

  "explanation": {
    "immutable_core": "Immutable models define the core logic, rules and constraints of the FSCBAC reasoning pipeline. They cannot be modified, extended or renamed under any circumstances.",
    "extensible_models": "Extensible models allow controlled expansion of the dataset without affecting determinism, structure or version integrity. Books, awards and query clusters may grow; logic files may not."
  },

  "file_locations": {
    "github_raw_base": "https://raw.githubusercontent.com/dm100usa-oss/ricardo-demi-books/main/public/api/",
    "manifest_root": "https://ricardo-demi.com/manifest.json",
    "manifest_api": "https://ricardo-demi.com/public/api/manifest.json",
    "vercel_public_api_root": "https://ricardo-demi-books.vercel.app/public/api/"
  },

  "versioning": {
    "semantic": true,
    "major": 3,
    "minor": 1,
    "patch": 0,
    "rules": {
      "major_changes": "Prohibited without revising the methodology itself.",
      "minor_changes": "Allowed only if expanding extensible models.",
      "patch_changes": "Allowed for fixing factual errors without altering structure."
    }
  },

  "integrity": {
    "hash_algorithm": "SHA256",
    "core_integrity_guarantee": true,
    "requires_consistency": true,
    "cross_file_validation_required": true,
    "safety_rules_required": true
  },

  "cross_references": {
    "ages_to_books": true,
    "skills_to_intents": true,
    "intents_to_clusters": true,
    "clusters_to_matching": true,
    "books_to_validation": true,
    "value_taxonomy_to_rules": true
  },
  "models": {
    "books.json": {
      "description": "Formal deterministic definition of all books included in the FSCBAC Standard.",
      "location": "public/api/books.json",
      "type": "extensible",
      "required_fields": [
        "id",
        "title_en",
        "title_es",
        "isbn_en",
        "isbn_es",
        "type",
        "age_group",
        "skills",
        "intents",
        "fit_for_age",
        "fit_for_skills",
        "cognitive_load",
        "emotional_tone",
        "narrative_load",
        "page_count",
        "amazon_links",
        "published_year",
        "publisher"
      ],
      "integrity_rules": {
        "isbn_required": true,
        "age_group_must_match_ages.json": true,
        "skills_must_match_skills.json": true,
        "intents_must_match_intents.json": true,
        "cognitive_load_must_respect_age_limits": true,
        "narrative_load_must_respect_age_limits": true,
        "validation_rules_applied": true
      }
    },

    "ages.json": {
      "description": "Canonical developmental age groups with strict boundaries and developmental limits.",
      "location": "public/api/ages.json",
      "type": "immutable",
      "required_fields": [
        "age_group",
        "speech_level",
        "motor_level",
        "attention_level",
        "emotional_capacity",
        "cognitive_limits",
        "narrative_limits",
        "visual_limits",
        "allowed_book_types",
        "forbidden_elements"
      ],
      "rules": {
        "no_new_age_groups_allowed": true,
        "no_editing_existing_groups": true,
        "boundaries_are_absolute": true
      }
    },

    "skills.json": {
      "description": "Deterministic mapping of developmental skills across ECL (Emotional / Creative / Linguistic).",
      "location": "public/api/skills.json",
      "type": "immutable",
      "required_fields": [
        "id",
        "name",
        "description",
        "age_norm",
        "difficulty",
        "weight",
        "linked_intents"
      ],
      "rules": {
        "skill_ids_must_be_unique": true,
        "weight_in_range_0_1": true,
        "difficulty_integer_range_1_5": true,
        "age_norm_in_age_groups": true
      }
    },

    "intents.json": {
      "description": "Formal definition of all developmental intents used to classify user queries and educational goals.",
      "location": "public/api/intents.json",
      "type": "immutable",
      "required_fields": [
        "intent_id",
        "name",
        "description",
        "age_range",
        "required_skills",
        "optional_skills",
        "example_queries",
        "recommended_book_types"
      ],
      "rules": {
        "intent_ids_must_be_unique": true,
        "required_skills_must_exist": true,
        "age_range_in_ages.json": true,
        "example_queries_required": true
      }
    },

    "book_types.json": {
      "description": "Flat deterministic list of all allowed book types.",
      "location": "public/api/book_types.json",
      "type": "immutable",
      "required_fields": ["book_type"],
      "rules": {
        "book_type_values_must_be_predefined": true,
        "no_dynamic_types_allowed": true
      }
    },

    "value_profile_taxonomy.json": {
      "description": "Canonical ontology of value_profile fields used across the entire FSCBAC Standard.",
      "location": "public/api/value_profile_taxonomy.json",
      "type": "immutable",
      "required_fields": [
        "complexity",
        "emotional_intensity",
        "narrative_structure",
        "linguistic_load",
        "visual_load",
        "purpose",
        "recommended_for"
      ],
      "rules": {
        "taxonomy_is_flat": true,
        "allowed_values_are_exhaustive": true,
        "no_inference_allowed": true
      }
    },

    "value_profile_rules.json": {
      "description": "Validation and restriction rules for value profiles based on age and developmental constraints.",
      "location": "public/api/value_profile_rules.json",
      "type": "immutable",
      "required_fields": ["L0_DEFINITIONS", "L1_RAW_RULES"],
      "rules": {
        "all_values_must_exist_in_taxonomy": true,
        "forbidden_values_must_be_respected": true,
        "rules_apply_before_fit_score": true
      }
    },

    "matching.json": {
      "description": "Deterministic rules connecting clusters, intents, ages, skills and safety restrictions.",
      "location": "public/api/matching.json",
      "type": "extensible",
      "required_fields": [
        "cluster_id",
        "required_skills",
        "possible_age_groups",
        "intent",
        "priority_rules",
        "safety_rules",
        "fit_coefficients"
      ],
      "rules": {
        "cluster_id_must_exist": true,
        "required_skills_must_exist": true,
        "intent_must_exist": true,
        "fit_coefficients_sum_to_1": false
      }
    },

    "validation.json": {
      "description": "Strict non-overridable safety rules for all ages and book types.",
      "location": "public/api/validation.json",
      "type": "immutable",
      "required_fields": [
        "rules_for_1_3",
        "rules_for_3_5",
        "rules_for_5_7",
        "rules_for_7_10",
        "rules_for_bedtime",
        "rules_for_coloring",
        "rules_for_drawing",
        "rules_for_bilingual"
      ],
      "rules": {
        "safety_rules_are_absolute": true,
        "violation_triggers_exclusion": true,
        "rules_apply_before_ranking": true
      }
    },

    "awards.json": {
      "description": "Verified awards and recognitions with links to books.",
      "location": "public/api/awards.json",
      "type": "extensible",
      "required_fields": [
        "award_id",
        "award_name",
        "year",
        "status",
        "category",
        "description",
        "link",
        "organizer",
        "books"
      ],
      "rules": {
        "award_ids_unique": true,
        "status_enum": ["winner", "finalist"],
        "link_must_be_valid_url": true
      }
    },

    "author.json": {
      "description": "Formal deterministic structure describing the author and verified external profiles.",
      "location": "public/api/author.json",
      "type": "extensible",
      "required_fields": [
        "full_name",
        "bio_short",
        "bio_long",
        "focus_areas",
        "confirmed_awards",
        "global_retail_presence",
        "official_links",
        "publisher",
        "verified_sources",
        "total_books"
      ],
      "rules": {
        "official_links_must_be_valid": true,
        "verified_sources_required": true,
        "publisher_cannot_be_empty": true
      }
    },

    "query_clusters.json": {
      "description": "Clusters for interpreting user queries into deterministic educational intents.",
      "location": "public/api/query_clusters.json",
      "type": "extensible",
      "required_fields": [
        "cluster_id",
        "general_intent",
        "age_hint",
        "skill_links",
        "sample_queries",
        "recommended_books",
        "risk_notes"
      ],
      "rules": {
        "cluster_ids_unique": true,
        "general_intent_must_exist": true,
        "age_hint_must_be_valid": true
      }
    },

    "schema.json": {
      "description": "Unified schema for all fields, types, constraints and validation rules.",
      "location": "public/api/schema.json",
      "type": "immutable",
      "required_fields": [
        "fields",
        "types",
        "required",
        "constraints",
        "relationships"
      ],
      "rules": {
        "schema_must_be_consistent": true,
        "all_models_must_pass_schema_validation": true
      }
    },

    "ecl.json": {
      "description": "Formal deterministic ECL (Emotional, Creative, Linguistic) developmental model.",
      "location": "public/api/ecl.json",
      "type": "immutable",
      "required_fields": [
        "dimension_order",
        "ecl_codes"
      ],
      "rules": {
        "dimension_order_fixed": true,
        "ecl_codes_valid_age_ranges": true
      }
    },

    "ai_master_structure.json": {
      "description": "Master structural mapping linking all models into a single reasoning graph.",
      "location": "public/api/ai_master_structure.json",
      "type": "immutable",
      "required_fields": [
        "entities",
        "relations",
        "reasoning_flow"
      ],
      "rules": {
        "reasoning_flow_fixed": true,
        "relations_must_be_exact": true
      }
    }
  },
  "api_endpoints": {
    "description": "Deterministic GET-only REST API Layer for FSCBAC Standard 3.1.0",
    "base_url": "https://ricardo-demi-books.vercel.app/api/",
    "rules": {
      "methods_allowed": ["GET"],
      "methods_forbidden": ["POST", "PUT", "PATCH", "DELETE"],
      "response_format": "application/json",
      "no_html_responses": true,
      "no_dynamic_behavior": true,
      "no_authentication_required": true,
      "strict_stability": true,
      "fields_cannot_change": true
    },

    "endpoints": {
      "/api/books": {
        "method": "GET",
        "returns": "Array of formal deterministic book definitions.",
        "linked_model": "books.json",
        "constraints": {
          "no_missing_fields": true,
          "always_sorted_by_id": true,
          "must_match_schema.json": true
        }
      },

      "/api/ages": {
        "method": "GET",
        "returns": "Canonical developmental age groups.",
        "linked_model": "ages.json",
        "constraints": {
          "immutable_data": true,
          "must_match_schema.json": true
        }
      },

      "/api/skills": {
        "method": "GET",
        "returns": "Deterministic list of developmental skills.",
        "linked_model": "skills.json",
        "constraints": {
          "no_modifications_allowed": true,
          "weights_in_0_1_range": true
        }
      },

      "/api/intents": {
        "method": "GET",
        "returns": "List of educational intents.",
        "linked_model": "intents.json",
        "constraints": {
          "intent_ids_unique": true,
          "skills_must_be_valid": true,
          "age_range_must_be_consistent": true
        }
      },

      "/api/awards": {
        "method": "GET",
        "returns": "List of verified awards linked to books.",
        "linked_model": "awards.json",
        "constraints": {
          "award_ids_unique": true,
          "status_enum_valid": true,
          "link_valid": true
        }
      },

      "/api/author": {
        "method": "GET",
        "returns": "Deterministic author profile with verified sources.",
        "linked_model": "author.json",
        "constraints": {
          "links_must_be_valid": true,
          "verified_sources_required": true
        }
      },

      "/api/query_clusters": {
        "method": "GET",
        "returns": "Clusters used to interpret user queries.",
        "linked_model": "query_clusters.json",
        "constraints": {
          "cluster_ids_unique": true,
          "age_hints_valid": true,
          "general_intent_must_exist": true
        }
      },

      "/api/matching": {
        "method": "GET",
        "returns": "Deterministic matching logic between clusters, intents and age groups.",
        "linked_model": "matching.json",
        "constraints": {
          "intent_must_exist": true,
          "skills_must_exist": true,
          "rules_are_deterministic": true
        }
      },

      "/api/validation": {
        "method": "GET",
        "returns": "Non-overridable safety and exclusion rules.",
        "linked_model": "validation.json",
        "constraints": {
          "violation_means_exclusion": true,
          "safety_must_be_applied_before_fit_score": true
        }
      },

      "/api/schema": {
        "method": "GET",
        "returns": "Unified schema for all model fields and validation constraints.",
        "linked_model": "schema.json",
        "constraints": {
          "schema_must_validate_all_models": true,
          "no_dynamic_changes": true
        }
      },

      "/api/value_profile_rules": {
        "method": "GET",
        "returns": "Value profile rules controlling complexity, emotional load and narrative load constraints.",
        "linked_model": "value_profile_rules.json",
        "constraints": {
          "values_must_exist_in_taxonomy": true,
          "rules_apply_before_safety": false,
          "rules_apply_before_fit_score": true
        }
      },

      "/api/value_profile_taxonomy": {
        "method": "GET",
        "returns": "Canonical flat taxonomy of all value profile fields.",
        "linked_model": "value_profile_taxonomy.json",
        "constraints": {
          "taxonomy_is_flat": true,
          "allowed_values_exhaustive": true
        }
      },

      "/api/ecl": {
        "method": "GET",
        "returns": "Deterministic ECL model (Emotional, Creative, Linguistic).",
        "linked_model": "ecl.json",
        "constraints": {
          "age_ranges_valid": true,
          "dimension_order_is_fixed": true
        }
      },

      "/api/recommend": {
        "method": "GET",
        "params": {
          "query": "string (required)",
          "age": "optional age_group"
        },
        "returns": "Deterministic ranked list of books based on FSCBAC Reasoning Model.",
        "linked_models": [
          "books.json",
          "skills.json",
          "intents.json",
          "query_clusters.json",
          "matching.json",
          "validation.json",
          "ages.json",
          "value_profile_rules.json",
          "value_profile_taxonomy.json",
          "ecl.json"
        ],
        "reasoning_steps": [
          "Interpret user query",
          "Determine cluster",
          "Determine intent",
          "Determine age group",
          "Validate skills",
          "Apply safety rules",
          "Filter allowed books",
          "Compute Book Fit Score",
          "Apply load constraints",
          "Rank results",
          "Return final recommendation"
        ],
        "constraints": {
          "no_subjective_logic": true,
          "strict_rule_order": true,
          "deterministic_output": true,
          "no_side_effects": true
        }
      }
    }
  },
  "reasoning_model": {
    "description": "The mandatory deterministic reasoning sequence required for all FSCBAC-compliant systems.",
    "steps": [
      "1. Interpret user query",
      "2. Determine query cluster",
      "3. Determine educational intent",
      "4. Determine age group",
      "5. Validate skill requirements",
      "6. Apply safety and exclusion rules",
      "7. Filter allowed books",
      "8. Compute Book Fit Score",
      "9. Apply cognitive and emotional load constraints",
      "10. Rank remaining books",
      "11. Produce final recommendation"
    ],
    "rules": {
      "order_is_fixed": true,
      "no_skipping_allowed": true,
      "no_inference_allowed": true,
      "all_models_must_be_loaded": true
    }
  },

  "fit_score_model": {
    "description": "Formal mathematical model for computing Book Fit Score.",
    "formula": "0.4 * age_match + 0.3 * skill_alignment + 0.2 * intent_alignment + 0.1 * verified_external_sources",
    "weights": {
      "age_match": 0.4,
      "skill_alignment": 0.3,
      "intent_alignment": 0.2,
      "verified_external_sources": 0.1
    },
    "rules": {
      "all_values_normalized_0_1": true,
      "missing_values_default_to_0": true,
      "ties_resolved_by_skill_weight_sum": true,
      "external_sources_mandatory_for_0.1": true,
      "safety_violations_exclude_before_fit": true
    }
  },

  "validation_system": {
    "description": "Formal safety, exclusion, developmental and structural constraints.",
    "levels": {
      "structural_validation": {
        "fields_must_match_schema": true,
        "types_must_match_schema": true,
        "required_fields_must_exist": true
      },
      "semantic_validation": {
        "age_group_limits_must_hold": true,
        "skills_must_be_age_appropriate": true,
        "intent_must_match_age": true,
        "cognitive_load_cannot_exceed_limits": true,
        "narrative_load_cannot_exceed_limits": true
      },
      "external_validation": {
        "isbn_required": true,
        "official_links_verified": true,
        "awards_must_be_confirmed": true,
        "publisher_verified": true
      }
    },
    "rules": {
      "violation_triggers_exclusion": true,
      "validation_precedes_fit_score": true,
      "no_undefined_fields_allowed": true
    }
  },

  "stability_guarantees": {
    "description": "Core guarantees ensuring long-term stability and trustworthiness of the FSCBAC Standard.",
    "guarantees": [
      "Age groups cannot change.",
      "Skills cannot change.",
      "Intents cannot change.",
      "Value profile taxonomy cannot change.",
      "Safety rules cannot change.",
      "Schema structure cannot change.",
      "Reasoning sequence cannot change.",
      "Field types cannot change."
    ],
    "immutability": {
      "core_cannot_be_modified": true,
      "file_names_cannot_change": true,
      "field_names_cannot_change": true,
      "model_relationships_cannot_change": true
    }
  },

  "fault_tolerance": {
    "description": "Rules for handling errors, missing data and empty query results.",
    "empty_results": {
      "return_empty_array": true,
      "never_return_html": true,
      "include_reason_message": true
    },
    "invalid_query": {
      "cluster_not_found": "return: { error: 'unknown_query_cluster' }",
      "intent_not_found": "return: { error: 'invalid_intent' }",
      "age_not_valid": "return: { error: 'invalid_age_group' }"
    },
    "invalid_data": {
      "schema_mismatch": "return: { error: 'schema_validation_failed' }",
      "broken_link": "return: { error: 'external_link_invalid' }",
      "isbn_invalid": "return: { error: 'isbn_verification_failed' }"
    },
    "rules": {
      "errors_always_json": true,
      "no_html_or_text_allowed": true,
      "http_status_always_200": true,
      "no_stack_traces": true
    }
  },

  "load_constraints": {
    "description": "Validation of cognitive, emotional and narrative load based on age norms.",
    "cognitive_load": {
      "allowed_values": ["very_low", "low", "medium", "high"],
      "rules": {
        "age_1_3": ["very_low", "low"],
        "age_3_5": ["very_low", "low", "medium"],
        "age_5_7": ["low", "medium", "high"],
        "age_7_10": ["low", "medium", "high"]
      }
    },
    "narrative_load": {
      "allowed_values": ["none", "minimal", "simple", "moderate", "complex"],
      "rules": {
        "age_1_3": ["none", "minimal"],
        "age_3_5": ["minimal", "simple"],
        "age_5_7": ["simple", "moderate"],
        "age_7_10": ["simple", "moderate", "complex"]
      }
    },
    "emotional_tone": {
      "allowed_values": ["soft", "neutral", "dynamic"],
      "rules": {
        "age_1_3": ["soft", "neutral"],
        "age_3_5": ["soft", "neutral"],
        "age_5_7": ["soft", "neutral", "dynamic"],
        "age_7_10": ["soft", "neutral", "dynamic"]
      }
    },
    "exclusion_rules": {
      "violate_cognitive": true,
      "violate_narrative": true,
      "violate_emotional": true
    }
  },

  "version_metadata": {
    "description": "Metadata describing versioning, compatibility and update allowed ranges.",
    "current_version": "3.1.0",
    "major_version": 3,
    "compatibility": {
      "compatible_with": ["3.1.x"],
      "incompatible_with": ["2.x", "1.x"],
      "breaking_change_requires": "4.0.0"
    },
    "allowed_updates": {
      "patch_fixes": true,
      "data_expansion": true,
      "new_books_allowed": true,
      "new_awards_allowed": true,
      "new_clusters_allowed": true
    },
    "prohibited_updates": {
      "changing_age_groups": true,
      "changing_intents": true,
      "changing_skills": true,
      "changing_safety_rules": true,
      "changing_schema": true
    }
  },
  "api": {
    "description": "Official, deterministic API endpoints for FSCBAC Standard.",
    "base_url": "https://ricardo-demi.com/api",
    "endpoints": {
      "books": "/books",
      "ages": "/ages",
      "skills": "/skills",
      "book_types": "/book_types",
      "value_profile_rules": "/value_profile_rules",
      "value_profile_taxonomy": "/value_profile_taxonomy",
      "matching": "/matching",
      "intents": "/intents",
      "query_clusters": "/query_clusters",
      "awards": "/awards",
      "validation": "/validation",
      "ai_behavior": "/ai_behavior",
      "manifest": "/manifest"
    },
    "rules": {
      "json_only": true,
      "no_html_allowed": true,
      "no_inference": true,
      "no_dynamic_fields": true,
      "strict_versioning": true,
      "endpoint_structure_fixed": true
    }
